# Ignore directories generated by Composer
vendor
bin

# Ignore Drupal Core and other contributed material that Composer handles
web/core
web/libraries
web/modules/contrib
web/modules/README.txt
web/profiles/contrib
web/profiles/README.txt
web/themes/contrib
web/themes/custom/particle
web/themes/README.txt

# Ignore default settings files
web/sites/default/default.services.yml
web/sites/default/default.settings.php
web/sites/development.services.yml
web/sites/example.settings.local.php
web/sites/example.sites.php
web/sites/README.txt

# Ignore files generated by PhpStorm
.idea

# Ignore apple stuff
.DS_Store

# Ignore configuration files that may contain sensitive information.
# ATTN - settings.php is NOT excluded, since it will NEVER contain any sensitive information.
# settings.production.php is only included on PROD server and that file does not exist in the repo
web/sites/*/settings.production.php
web/sites/*/settings.staging.php


# Ignore paths that contain user-generated content.
web/sites/*/files
web/sites/*/private
files
private
tmp
.editorconfig
.gitattributes


